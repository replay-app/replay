<?xml version="1.0" encoding="UTF-8"?>
<interface>

<template class="RpyNavigationSidebarItem" parent="GtkListBoxRow">

  <property name="child">
    <object class="GtkBox">

      <child>
        <object class="GtkImage" id="icon_widget">
          <property name="icon-name"
                    bind-source="RpyNavigationSidebarItem"
                    bind-property="icon-name"
                    bind-flags="sync-create" />
        </object>
      </child>

      <child>
        <object class="GtkLabel" id="label_widget">
          <property name="label"
                    bind-source="RpyNavigationSidebarItem"
                    bind-property="label"
                    bind-flags="sync-create" />
        </object>
      </child>

    </object>
  </property>

</template>

</interface>
