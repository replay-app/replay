<?xml version="1.0" encoding="UTF-8"?>
<interface>

<template class="RpyShellWindow" parent="AdwApplicationWindow">
  <property name="default-width">1020</property>
  <property name="default-height">720</property>
  <property name="width-request">360</property>
  <property name="height-request">294</property>

  <property name="content">
    <object class="AdwMultiLayoutView">

      <child type="content">
        <object class="AdwBin" />
      </child>

      <child>
        <object class="AdwLayout">
          <property name="name">desktop</property>

          <property name="content">
            <object class="RpyShellDesktopLayout">
              <property name="model"
                        bind-source="RpyShellWindow"
                        bind-property="model"
                        bind-flags="sync-create" />
            </object>
          </property>

        </object>
      </child>

    </object>
  </property>

</template>

</interface>
